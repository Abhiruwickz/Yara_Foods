{
  "expo": {
    "name": "Yara_Foods",
    "slug": "yara-foods",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Yara_Foods.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.casioabx.yarafoods"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6bbb5da2-0095-4f4b-ab0f-0128cc2c1936"
      }
    },
    "owner": "casioabx"
  }
}
